{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "start": {
      "cache": false,
      "dependsOn": [
        "generate:licenses"
      ]
    },
    "clean": {
      "cache": false
    },
    "test": {},
    "build": {
      "dependsOn": [
        "test",
        "generate:licenses",
        "^build"
      ],
      "outputs": [
        ".vercel/output/static/**"
      ]
    },
    "generate:licenses": {
      "dependsOn": [
        "^generate:licenses"
      ],
      "outputs": [
        "./apps/web/__generated__/licenses.json"
      ]
    }
  }
}
